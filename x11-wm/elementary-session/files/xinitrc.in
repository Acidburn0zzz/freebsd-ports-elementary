#!/bin/sh

#export LC_CTYPE="fr_FR.UTF-8"
#export LANG="fr_FR.UTF-8"

# Probably bad idea
rm $HOME/*.core

[ -f $HOME/.xprofile ] && . $HOME/.xprofile

%%PREFIX%%/libexec/gvfsd-trash &
[ -e "$XDG_CONFIG_HOME/autostart/plank.desktop" ] || %%PREFIX%%/bin/plank &
exec %%PREFIX%%/bin/ck-launch-session %%PREFIX%%/bin/gnome-session --builtin --session=pantheon
