#!/bin/sh
#
# Use only .xinitrc script from a console (startx or xdm).
#

#export LC_CTYPE="fr_FR.UTF-8"
#export LANG="fr_FR.UTF-8"

[ -f $HOME/.xprofile ] && . $HOME/.xprofile

# Usually loaded when 'io.elementary.files is opened
#%%PREFIX%%/libexec/gvfsd-trash &
[ -f "$XDG_CONFIG_HOME/autostart/plank.desktop" ] || plank &
exec ck-launch-session gnome-session --builtin --session=pantheon
